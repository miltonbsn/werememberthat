<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<ui:style>
	
	</ui:style>
	
	<g:HTMLPanel styleName="content">
		<div class="wrapper">
			<!-- Aqui você adiciona as tarefas que tem que fazer -->
			<g:HTMLPanel styleName="newReminder cf" >
				<h2>Add new stuff!</h2>
				<div class="entry">
					<g:TextBox ui:field="descTextBox"/>
					<g:Anchor ui:field="addButton" text="Save" />
					</div><!-- End .entry -->
			</g:HTMLPanel>
			
			<!-- Aqui você vê as tarefas que devem ser feitas -->
			<h2>Dont'forget it!</h2>
			<g:HTMLPanel ui:field="remindersList"/>
			<!-- Aqui você vê as tarefas que já foram feitas -->
			<g:HTMLPanel ui:field="doneList"/>
		</div><!-- End .wrapper -->
	</g:HTMLPanel><!-- End .content -->
	
</ui:UiBinder> 